  justify-content: center;
  align-items: center;
  background-color: #88bb1e;
  border-radius: 40rpx;
  color: #000000;
}
page .search_bar navigator icon {
  padding-right: 5rpx;
}
page .search_bar .campus {
  width: 50%;
  height: 80rpx;
  font-size: 28rpx;
