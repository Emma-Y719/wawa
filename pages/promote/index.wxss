  text-align: center;
  align-items: center;
  background-color: #88bb1e;
  border-radius: 40rpx;
  margin-right: 40rpx;
}
page .description .contant-pic {
  width: 90%;
  margin-top: 2%;
  margin-left: 5%;
}
page .description .contant-pic .pics-list {
  width: 160rpx;
  height: 160rpx;
  float: left;
  margin-right: 10rpx;
  background-color: #adabab;
  border-radius: 10rpx;
}
page .description .contant-pic .pics-list .uploadImg {
  border-radius: 10rpx;
  width: 160rpx;
  height: 160rpx;
}
page .description .contant-pic .pics-list .uploadImg-error {
  height: 40rpx;
  width: 40rpx;
  position: relative;
  top: -165rpx;
  left: 120rpx;
}
page .description .contant-pic .up {
  width: 160rpx;
  height: 160rpx;
