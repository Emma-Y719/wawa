page .typeContainer {
  display: flex;
  margin-top: 2%;
  margin-left: 5%;
  width: 90%;
  padding: 5rpx 5rpx 5rpx 5rpx;
  background-color: #88bb1e;
  height: 60rpx;
  border-radius: 30rpx;
}
page .typeContainer .type {
  width: 34%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  align-content: center;
}
page .typeContainer .type text {
  font-size: 28rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 34rpx;
  color: #333333;
}
page .typeContainer .selected {
  width: 32%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  align-content: center;
  background-color: #f2f2f2;
  border-radius: 30rpx;
}
page .typeContainer .selected text {
  font-size: 28rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 34rpx;
  color: #333333;
}
