page .typeContainer {
  display: flex;
  position: fixed;
  top: 2%;
  left: 5%;
  width: 90%;
  padding: 18rpx 12rpx 8rpx;
  background-color: #f2f2f2;
  border-radius: 20rpx;
}
page .typeContainer .type {
  width: 34%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
page .typeContainer .type .img {
  display: flex;
}
page .typeContainer .type .img .tixing {
  width: 20rpx;
}
page .typeContainer .type .img image {
  width: 80rpx;
  height: 80rpx;
}
page .typeContainer .type text {
  margin-top: 5rpx;
  margin-bottom: 5rpx;
  font-size: 28rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 34rpx;
  color: #333333;
}
page .chatList {
  margin-top: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
page .chatList .chat {
  margin-top: 30rpx;
  background-color: #fcfcfc;
  border-radius: 20rpx;
  display: flex;
}
page .chatList .chat .tixing {
  width: 20rpx;
}
page .chatList .chat .userPic {
  width: 90rpx;
  height: 90rpx;
  border-radius: 45rpx;
}
page .chatList .chat navigator {
  display: flex;
}
page .chatList .chat navigator .middle {
  margin-left: 20rpx;
  width: 330rpx;
}
page .chatList .chat navigator .middle .title .chat_name {
  font-size: 28rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 34rpx;
  color: #333333;
}
