  align-items: center;
  font-size: 20rpx;
}
page .chatList .chat navigator .middle .chat_message {
  margin-top: 5rpx;
  font-size: 22rpx;
  color: #6b6b6b;
}
page .chatList .chat navigator .date {
  margin-right: 10rpx;
