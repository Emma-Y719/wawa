page {
  box-sizing: border-box;
  color: #333;
  width: 100%;
  background: linear-gradient(to bottom, #96c03c, #ffffff);
  /* 渐变色背景样式 */
  background-repeat: no-repeat;
  font-family: "Bitstream Vera Serif Bold";
  padding: 14rpx;
  height: 100%;
}
page .background {
  width: 89%;
  margin-left: 5.5%;
  height: 20%;
  z-index: 9;
  background-color: #8AC186;
  position: absolute;
  border-radius: 10rpx;
  border-top: 3px solid #A54B09;
  border-right: 3px solid #A54B09;
  border-left: 3px solid #A54B09;
  top: 12%;
  left: 0;
  z-index: -1;
  /* 将背景图片置于底层 */
}
page .custom-nav-bar {
  display: flex;
  height: 10%;
  /* 设置导航栏的高度 */
  position: relative;
}
page .custom-nav-bar .text {
  position: absolute;
  bottom: 10%;
  /* 距离底部的位置 */
  left: 50%;
  transform: translateX(-50%);
  font-size: 30rpx;
  color: #333;
}
page .hello {
  color: #333;
  font-size: 30rpx;
  font-weight: 500;
  margin-left: 84rpx;
  color: #ffffff;
  font-size: 34rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 40rpx;
  margin-top: 8%;
}
page .introduce {
  color: #cf1919;
  width: 450rpx;
  font-size: 24rpx;
  font-weight: 700;
  margin-left: 30rpx;
  margin-top: 15rpx;
}
page .info {
  background-color: #fff;
  border-radius: 25rpx;
  height: 200rpx;
  padding: 5rpx;
  margin-top: 15%;
  margin-left: 4%;
  width: 92%;
}
page .info .info-content {
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  height: 50%;
  padding: 20rpx;
  position: relative;
}
page .info .info-content .title {
  display: flex;
  flex-direction: row;
  align-items: center;
}
page .info .info-content .title .location {
  color: #333;
  font-size: 30rpx;
  font-weight: bold;
}
page .info .info-content .title .location .line {
  display: inline-block;
  width: 6px;
  height: 6px;
  border-bottom: 1px solid #000000;
  border-right: 1px solid #000000;
  transform: rotate(45deg);
  margin-bottom: 6rpx;
}
page .info .info-content .type {
  margin-left: 5%;
  width: 45%;
  font-size: 37rpx;
  font-weight: 600;
  color: #000;
  display: flex;
  align-items: center;
}
page .info .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  background: #87c03f;
  border-radius: 30rpx;
  font-size: 30rpx;
  font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
  font-weight: bold;
  color: #fff;
  width: 60%;
  margin-left: 3%;
  margin-top: 15rpx;
  height: 50rpx;
}
page .info .btn image {
  width: 30rpx;
  margin-right: 20rpx;
}
page .logo {
  top: 15%;
  bottom: 20%;
  height: 340rpx;
  position: absolute;
  left: 435rpx;
  width: 330rpx;
}
page .index_bigType {
  margin-top: 30rpx;
  display: flex;
  width: 92%;
  margin-left: 4%;
  flex-direction: row;
  height: 250rpx;
}
page .index_bigType .rows {
  width: 68%;
}
page .index_bigType .rows .bigTypeRow {
  display: flex;
  height: 120rpx;
  align-items: center;
  margin-bottom: 10rpx;
}
page .index_bigType .rows .bigTypeRow navigator {
  flex: 1;
  height: 100%;
  margin-right: 10rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
page .index_bigType .rows .bigTypeRow navigator image {
  margin-top: 8rpx;
  width: 70rpx;
  height: 70rpx;
}
page .index_bigType .rows .bigTypeRow navigator text {
  margin-top: 5rpx;
  margin-bottom: 5rpx;
  text-align: center;
  font-size: 22rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 26rpx;
  color: #979797;
}
page .index_bigType .hotIntro {
  width: 32%;
  border-radius: 10rpx;
  background-color: #fafafa;
  align-items: center;
  justify-content: center;
}
page .index_bigType .hotIntro .titles .icon {
  width: 38%;
  margin-top: 20rpx;
  margin-left: 31%;
}
page .index_bigType .hotIntro .titles .middle .uni_title {
  font-size: 22rpx;
  font-weight: 400;
  text-align: center;
  color: #000000;
}
page .index_bigType .hotIntro .titles .middle .cam_title {
  font-size: 22rpx;
  font-weight: 400;
  text-align: center;
  color: #000000;
}
page .index_bigType .hotIntro .titles .bottom {
  height: 80rpx;
  display: flex;
  align-items: center;
  text-align: center;
  margin-left: 30rpx;
}
page .index_bigType .hotIntro .titles .bottom .btn {
  margin-top: 10rpx;
  display: flex;
  align-items: center;
  text-align: center;
  border-radius: 30rpx;
  font-size: 28rpx;
  font-weight: bold;
  font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
  color: #3a3a3a;
}
page .index_bigType .hotIntro .titles .bottom .enter {
  margin-top: 10rpx;
  width: 40rpx;
}
page .display_line {
  width: 92%;
  margin-left: 4%;
  margin-top: 30rpx;
  height: 200rpx;
  display: flex;
  justify-content: space-between;
}
page .display_line .container_ {
  border-radius: 10rpx;
  background-color: #fff;
  height: 100%;
  width: 32%;
}
page .display_line .container_ .chat_row {
  width: 88%;
  margin-left: 6%;
  margin-top: 8%;
  height: 25%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
page .display_line .container_ .chat_row image {
  width: 45rpx;
  border-radius: 25rpx;
}
page .display_line .container_ .display {
  width: 88%;
  margin-top: 8%;
  height: 58%;
  margin-left: 6%;
  display: flex;
  flex-direction: column;
}
page .display_line .container_ .display .row {
  display: flex;
  height: 60%;
}
page .display_line .container_ .display .row image {
  width: 45rpx;
  height: 45rpx;
  border-radius: 25rpx;
}
page .display_line .container_ .display .row .title {
  margin-left: 10rpx;
  font-size: 26rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 30rpx;
  color: #979797;
  margin-top: 3%;
}
page .display_line .container_ .bottom {
  width: 88%;
  margin-left: 6%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 5%;
  height: 50rpx;
}
page .display_line .container_ .bottom .icon {
  height: 30rpx;
  width: 30rpx;
}
page .display_line .container_ .bottom .content_text {
  margin-left: 5rpx;
  bottom: 20rpx;
  /* 距离底部的位置 */
  text-align: center;
  font-size: 26rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 30rpx;
  color: #979797;
}
page .display_line ad-custom {
  margin-left: 10rpx;
}
page .display {
  margin-top: 10rpx;
  display: flex;
  width: 92%;
  margin-left: 4%;
  height: 180rpx;
  justify-content: space-between;
}
page .display .index_swiper {
  width: 32%;
  border-radius: 10rpx;
  background-color: #fff;
}
page .display .index_swiper swiper {
  height: 100%;
  border-radius: 10rpx;
}
page .display .index_swiper swiper swiper-item {
  height: 100%;
  border-radius: 10rpx;
}
page .display .index_swiper swiper swiper-item navigator {
  display: flex;
  height: 100%;
  border-radius: 10rpx;
}
page .display .index_swiper swiper swiper-item navigator image {
  border-radius: 10rpx;
  width: 100%;
  height: 100%;
}
page .display .index_swiper swiper swiper-item navigator .swiperinfo {
  display: flex;
  flex-direction: column;
  width: 50%;
  margin-left: 20rpx;
  margin-top: 15rpx;
}
page .display .index_swiper swiper swiper-item navigator .swiperinfo .title {
  width: 50%;
  font-size: 24rpx;
  font-weight: 500;
  white-space: nowrap;
  /* 不换行 */
  overflow: hidden;
  /* 超出部分隐藏 */
  text-overflow: ellipsis;
  /* 使用省略号显示 */
}
page .display .index_swiper swiper swiper-item navigator .swiperinfo .storage {
  display: flex;
  align-items: center;
  justify-content: center;
}
page .display .index_swiper swiper swiper-item navigator .swiperinfo .storage image {
  width: 10%;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: start;
}
page .display .index_swiper swiper swiper-item navigator .swiperinfo .storage text {
  font-size: 20rpx;
  white-space: nowrap;
  /* 不换行 */
  overflow: hidden;
  /* 超出部分隐藏 */
  text-overflow: ellipsis;
  /* 使用省略号显示 */
}
page .display .index_swiper swiper swiper-item navigator .swiperinfo .price {
  font-size: 22rpx;
}
page .display .index_swiper swiper swiper-item navigator .swiperinfo .swiper_university {
  margin-top: 5rpx;
  font-size: 20rpx;
}
page .display .index_swiper swiper swiper-item navigator .swiperinfo .swiper_campus {
  font-size: 20rpx;
}
page .display .map_container {
  width: 66%;
  border-radius: 10rpx;
  margin-left: 10rpx;
  overflow: hidden;
}
page .display .map_container .map {
  height: 100%;
  width: 100%;
}
page .commend {
  width: 92%;
  margin-left: 4%;
  margin-top: 40rpx;
}
page .commend .title {
  font-size: 26rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 30rpx;
  color: #000000;
  text-align: center;
}
page .commend .camloc {
  margin-top: 10rpx;
  font-size: 24rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 30rpx;
  color: #979797;
  text-align: center;
}
page .commend .products .product_list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 10rpx;
}
page .commend .products .product_list .product_card {
  margin-bottom: 40rpx;
  width: 48%;
  text-align: center;
  background-color: #ffffff;
  border-radius: 10rpx;
}
page .commend .products .product_list .product_card navigator image {
  width: 100%;
  height: 240rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  background-color: #fff;
  transition: transform 200ms;
}
page .commend .products .product_list .product_card navigator image:hover {
  transform: scale(1.01);
  box-shadow: -2px 8px 15px -12px #647878;
}
page .commend .products .product_list .product_card navigator .middle {
  height: 50rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  text-align: start;
  margin-left: 10rpx;
}
page .commend .products .product_list .product_card navigator .middle .product_name {
  font-size: 26rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 30rpx;
  color: #000000;
  width: 70%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
page .commend .products .product_list .product_card navigator .middle .product_price {
  font-size: 26rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 30rpx;
  color: #ff7a00;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-right: 10rpx;
}
page .commend .products .product_list .product_card .bottom {
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  text-align: start;
  margin-left: 10rpx;
  margin-top: 5rpx;
}
page .commend .products .product_list .product_card .bottom .locinfo {
  font-size: 20rpx;
  font-family: '.AppleSystemUIFont';
  line-height: 24rpx;
  color: #979797;
  width: 70%;
}
page .commend .products .product_list .product_card .bottom .operator {
  display: flex;
}
page .commend .products .product_list .product_card .bottom .operator image {
  background-color: #fafafa;
  margin-right: 10rpx;
  width: 45rpx;
}
page .bottom {
  height: 200rpx;
}
